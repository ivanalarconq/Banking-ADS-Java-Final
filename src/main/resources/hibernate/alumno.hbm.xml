<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="banking.users.domain.entity">
	<class name="Alumno" table="alumno">
		<id name="id" column="alumno_id">
			<generator class="native" />
		</id>
		<property name="name" column="alumno_name" />
		<property name="firstName" column="first_name" />
		<property name="lastName" column="last_name" />
		<property name="gender" column="gender" />
		<property name="emailAddress" column="email_address" />
		<property name="isActive" column="active" />
		<property name="createdAt" column="created_at_utc" />
		<property name="updatedAt" column="updated_at_utc" />
	</class>
</hibernate-mapping>